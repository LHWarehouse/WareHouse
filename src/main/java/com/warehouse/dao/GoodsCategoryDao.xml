<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.warehouse.dao.GoodsCategoryDao">
  <!-- 查询 -->
  	<select id="selectCategoryListByCode" 
  		parameterType="String" resultType="GoodsCategory">
  		SELECT * FROM goods_category WHERE category_type_code = #{value}
  	</select>
  	
  	<!-- 总条数 -->
  </mapper>